// ===================================================================
// Stdlib.Bool.not - Returns the inverse of a boolean
// ===================================================================

// Stdlib.Bool.not(true) = stdout="false\n"
// Stdlib.Bool.not(false) = stdout="true\n"

// With variable
// let b = true in Stdlib.Bool.not(b) = stdout="false\n"
// let b = false in Stdlib.Bool.not(b) = stdout="true\n"

// Double not
// Stdlib.Bool.not(Stdlib.Bool.not(true)) = stdout="true\n"
// Stdlib.Bool.not(Stdlib.Bool.not(false)) = stdout="false\n"

// ===================================================================
// Stdlib.Bool.and - Returns true if both a and b are true
// ===================================================================

