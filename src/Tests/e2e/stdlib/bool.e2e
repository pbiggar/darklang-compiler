// Stdlib.Bool.and(false, true) = stdout="false\n"
// Stdlib.Bool.and(false, false) = stdout="false\n"

// With expressions
// Stdlib.Bool.and(8 >= 5, 6 <= 8) = stdout="true\n"
// Stdlib.Bool.and(1 > 5, 2 < 8) = stdout="false\n"

