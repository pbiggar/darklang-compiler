// ===================================================================
// Tuple literals (from Dark dtuple.dark)
// ===================================================================

(1L, 2L) = (1, 2)
(1L, 2L, 3L) = (1, 2, 3)
(1L, 2L + 3L, 4L) = (1, 5, 4)

// No upper limit on tuple size
(1L, 2L, 3L, 4L, 5L, 6L, 7L, 8L, 9L) = (1, 2, 3, 4, 5, 6, 7, 8, 9)

